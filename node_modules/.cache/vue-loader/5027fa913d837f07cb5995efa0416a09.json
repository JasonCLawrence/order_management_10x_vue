\"\n                        @add=\"showModal\"\n                        @import=\"showImportModal()\"\n                        ref=\"table\"\n                    ></order-table>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\nimport OrderTable from './OrderTable'\nimport CreateOrderForm from './CreateOrderForm'\nimport ImportInvoicesForm from './ImportInvoicesForm'\n\nexport default {\n    name: 'orders',\n    components: {\n        OrderTable,\n        CreateOrderForm,\n        ImportInvoicesForm,\n    },\n    data: () => {\n        return {\n            addModal: false,\n            importModal: false,\n        }\n    },\n\n    methods: {\n        hideModal() {\n            this.addModal = false\n        },\n        showModal() {\n            this.addModal = true\n        },\n        hideImportModal() {\n            this.importModal = false\n        },\n        showImportModal() {\n            this.importModal = true\n        },\n        orderCreated(data) {\n            this.addModal = false\n            this.$refs.table.doSearch()\n\n            this.$swal({\n                title: 'Order Created',\n                type: 'success',\n            }).then(() => {\n                this.$router.push('/orders/' + data.id)\n            })\n        },\n    },\n}\n</script>\n<style>\n.invoice-import-modal {\n    max-width: 900px !important;\n    max-height: 500px !important;\n    margin-top: 8em;\n}\n</style>\n"]}]}