                  <div class=\"row\">\n                    <div class=\"col-lg-6\">\n                      <base-input alternative label=\"Name\" placeholder=\"Name\" v-model=\"model.name\" />\n                    </div>\n                    <div class=\"w-100\"></div>\n                    <div class=\"col-lg-6\">\n                      <base-input\n                        alternative\n                        label=\"Email address\"\n                        placeholder=\"jesse@example.com\"\n                        v-model=\"model.email\"\n                      />\n                    </div>\n                    <div class=\"w-100\"></div>\n                  </div>\n                </div>\n                <hr class=\"my-4\" />\n                <!-- Address -->\n                <h6 class=\"heading-small text-muted mb-4\">Location</h6>\n                <div class=\"pl-lg-4\">\n                  <div class=\"row\">\n                    <div class=\"col-md-12\">\n                      <base-input\n                        alternative\n                        label=\"Address\"\n                        placeholder=\"Address\"\n                        v-model=\"model.address\"\n                      />\n                    </div>\n                  </div>\n                  <div class=\"row\">\n                    <div class=\"col-lg-4\">\n                      <base-input alternative label=\"City\" placeholder=\"City\" v-model=\"model.city\" />\n                    </div>\n                    <div class=\"col-lg-4\">\n                      <div class=\"form-group has-label\">\n                        <label class=\"form-control-label\">\n                          Country\n                          <!---->\n                        </label>\n                        <!---->\n                        <country-list v-model=\"model.country\" />\n                        <!---->\n                        <!---->\n                      </div>\n                    </div>\n                    <div class=\"col-lg-4\">\n                      <base-input\n                        alternative\n                        label=\"Postal code\"\n                        placeholder=\"Postal code\"\n                        v-model=\"model.zipCode\"\n                      />\n                    </div>\n                  </div>\n                </div>\n                <div class=\"pl-lg-4\">\n                  <div class=\"row\">\n                    <div class=\"col-md-12 text-right\">\n                      <base-button\n                        type=\"primary\"\n                        @click=\"updateCompany()\"\n                        :disabled=\"submitting\"\n                      >{{ submitting?\"Saving Changes...\":\"Save Changes\" }}</base-button>\n                    </div>\n                  </div>\n                </div>\n              </form>\n            </template>\n          </card>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport api from '@/api/company'\nimport CountryList from '@/views/Company/CountryList'\n\nexport default {\n    name: 'company-settings',\n    components: { CountryList },\n    data() {\n        return {\n            model: {\n                name: 'My Company',\n                email: '',\n                street: '',\n                city: 'Kingston',\n                country: 'Jamaica',\n                zipCode: '',\n                about: '',\n            },\n            submitting: false,\n        }\n    },\n    mounted() {\n        api.fetch().then(result => {\n            let data = result.data.success.data\n            this.model = data\n        })\n    },\n    methods: {\n        updateCompany() {\n            this.submitting = true\n            api.update(this.model)\n                .then(res => {})\n                .finally(res => {\n                    this.submitting = false\n                })\n        },\n    },\n}\n</script>\n<style></style>\n"],"sourceRoot":"src/views"}]}